<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/css/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/font-awesome/css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/Ionicons/css/ionicons.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/dist/css/default.min.css') }}">
        <!-- AdminLTE Skins. We have chosen the skin-blue for this starter
    page. However, you can choose any other skin. Make sure you
    apply the skin class to the body tag so the changes take effect. -->
        <link rel="stylesheet" href="{{ asset('assets/css/dist/css/skins/skin-blue-light.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/common.css') }}">
    {% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>


<body class="hold-transition skin-blue-light sidebar-mini">
    <div class="wrapper">
    <!-- Main Header -->

        <header class="main-header">
            {% block header %}
                {% include 'default/header.html.twig' %}
            {% endblock %}
        </header>

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            {% block left_sidebar %}
                {% include 'default/left-sidebar.html.twig' %}
            {% endblock %}
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div class="col-md-9">
                <!-- Content Header (Page header) -->
                <section class="content-header">
                    {% block content_header %}
                        {% include 'default/content-header.html.twig' %}
                    {%  endblock %}
                </section>
                <!-- Main content -->
                <section class="content container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            {% block content %}{% endblock %}
                        </div>
                    </div>

                </section>
            </div>
            <div class="col-md-3 right-sidebar">
                <div class="box box-default box-solid info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Info</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="panel-default">
                            <div class="info-panel col-md-2 no-padding">
                                <div class="pull-left image">
                                    <img src="{{ asset('assets/css/dist/img/user2-160x160.jpg')}}" class="img-circle" alt="User Image"/>
                                </div>
                            </div>
                            <div class="info col-md-10 no-padding">
                                <p><span>Alexander Pierce</span> : Proprietaire </p>
                                <p>Ajouté le : 28/10/2015 à 16:00 </p>
                                <p>5,13 Mo, 5 Fichier(s), 1 Dossier(s)</p>
                            </div>
                        </div>
                        <div class="panel-default">
                            <div class="info-panel col-md-2 no-padding">
                                <div class="pull-left image">
                                    <i class="fa  fa-info-circle panel-title-img"></i>
                                </div>
                            </div>
                            <div class="info col-md-10 no-padding">
                                <div class="panel-title">Actualité <i class="fa  fa-plus-circle"></i></div>
                                <ul>
                                    <li><p><span>Alexander Pierce</span> : Proprietaire </p>
                                        <p>Ajouté le : 28/10/2015 à 16:00 </p>
                                    </li>
                                    <li><p><span>Alexander Pierce</span> : Proprietaire </p>
                                        <p>Ajouté le : 28/10/2015 à 16:00 </p>
                                    </li>
                                    <li><p><span>Alexander Pierce</span> : Proprietaire </p>
                                        <p>Ajouté le : 28/10/2015 à 16:00 </p>
                                    </li>
                                </ul>
                            </div>
                        </div>

                        <div class="panel-default">
                            <div class="info-panel col-md-2 no-padding">
                                <div class="pull-left image">
                                    <i class="fa fa-cog panel-title-img"></i>
                                </div>
                            </div>
                            <div class="info col-md-10 no-padding">
                                <div class="panel-title">Paramètres </div>
                            </div>
                        </div>

                    </div>
                    <!-- /.box-body -->
                </div>
                <div class="box box-default box-solid uploadable">
                    <div class="box-header with-border">
                        <h3 class="box-title"><i class="fa fa-download "></i>&nbsp;&nbsp;&nbsp;&nbsp;Importer des fichiers</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="panel-default">
                            <div class="info col-md-12 no-padding text-center ">
                                <div >
                                    <i class="fa fa-cloud-upload panel-title-img"></i>
                                    <p>Glisser & déposer des fichiers</p>
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
                <div class="box box-default box-solid">
                    <div class="box-header with-border">
                        <h3 class="box-title">Notification par email</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="panel-default">
                            <div class="form-group">
                                <div class="checkbox">
                                    <label>
                                        <input type="checkbox">Importation de fichier
                                    </label>
                                </div>
                                <div class="checkbox">
                                    <label>
                                        <input type="checkbox">Nouveau commentaire dans actualité
                                    </label>
                                </div>
                            </div>
                        </div>

                    </div>
                    <!-- /.box-body -->
                </div>
                <div class="box box-default box-solid">
                    <div class="box-header with-border">
                        <h3 class="box-title">Description</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="panel-default">
                            <input class="form-control" placeholder="Ajouter une description" type="text">
                        </div>

                    </div>
                    <!-- /.box-body -->
                </div>


            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

        <!-- Main Footer -->
        <footer class="main-footer">
            {% block footer %}
                {% include 'default/footer.html.twig' %}
            {% endblock %}
        </footer>

    </div>
<!-- ./wrapper -->
{% block javascripts %}
    <!-- jQuery 3 -->
    <script src="{{ asset('assets/js/jquery/dist/jquery.min.js') }}"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="{{ asset('assets/css/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <!-- AdminLTE App -->
    <script src="{{ asset('assets/js/js/default.min.js') }}"></script>
{% endblock %}
</body>
</html>
